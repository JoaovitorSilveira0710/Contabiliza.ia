{
  "name": "contabiliza-ia-frontend",
  "version": "1.0.0",
  "description": "Frontend para plataforma de gestão contábil inteligente",
  "private": true,
  "scripts": {
    "dev": "npx http-server . -p 3000 -c-1 --cors",
    "build:css": "npx tailwindcss -i ./src/styles/globals.css -o ./dist/output.css",
    "watch:css": "npx tailwindcss -i ./src/styles/globals.css -o ./dist/output.css --watch",
    "start": "npx http-server . -p 3000 -c-1"
  },
  "dependencies": {
    "chart.js": "^4.4.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.22",
    "http-server": "^14.1.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18"
  }
}
